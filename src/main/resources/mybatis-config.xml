        <typeAlias type="com.spring.model.OrderDTO" alias="OrderDTO"/>
        <typeAlias type="com.spring.model.Product_categoryDTO" alias="Product_categoryDTO"/>
        <typeAlias type="com.spring.model.Product_contentDTO" alias="Product_contentDTO"/>
        <typeAlias type="com.spring.model.ProductDTO" alias="ProductDTO"/>
        <typeAlias type="com.spring.model.ReviewDTO" alias="ReviewDTO"/>
        <typeAlias type="com.spring.model.Subscribe_userDTO" alias="Subscribe_userDTO"/>
        <typeAlias type="com.spring.model.SubscribeDTO" alias="SubscribeDTO"/>
