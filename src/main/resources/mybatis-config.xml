        <typeAlias type="com.spring.service.ProductDAO" alias="ProductDAO"/>
        <typeAlias type="com.spring.service.ReviewDAO" alias="ReviewDAO"/>
        <typeAlias type="com.spring.service.Subscribe_userDAO" alias="Subscribe_userDAO"/>
        <typeAlias type="com.spring.service.SubscribeDAO" alias="SubscribeDAO"/>
        <typeAlias type="com.spring.service.UserDAO" alias="UserDAO"/>
