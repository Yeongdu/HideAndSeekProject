        <typeAlias type="com.spring.service.EventDAO" alias="EventDAO"/>
        <typeAlias type="com.spring.service.OrderDAO" alias="OrderDAO"/>
        <typeAlias type="com.spring.service.Product_categoryDAO" alias="Product_categoryDAO"/>
